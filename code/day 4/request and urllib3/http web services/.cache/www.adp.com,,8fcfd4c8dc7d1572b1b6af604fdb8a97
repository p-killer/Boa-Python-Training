<script type="text/javascript">
if( !isEU() ) {
    params = window.params || {};

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
        var nshpr = getCookie("gtmStorage");
        if ( nshpr == "{\"nshpr\":\"1\"}" ) { window.params.chatnonshopper = "nonshopper"; } else { window.params.chatnonshopper = ''; }
        // console.log('chatnonshopper is: ' + params.chatnonshopper);

    function checkGA(Count){
      if(Count < 10){
        if(!window.ga){
          setTimeout(function(){checkGA(Count++);}, 1000);
          // console.log('current count is: ' + Count);
        }
        else {
          ga(function() {
            window.params.gClientId = ga.getAll()[0].get('clientId');
            window.params.gClientId = window.params.gClientId + '';
            // console.log('gClientId is: ' + params.gClientId);
          });
         if ( window.optimizely && window.optimizely.data && window.optimizely.data.state ) { 
      var o = window.optimizely.data.state;
      if (o.activeExperiments.length > 0) {
        var experimentId = o.activeExperiments[0];
        var variationIndex = o.variationMap[experimentId];
        var variationName = o.variationNamesMap[experimentId];
        window.params.LPIDcombined = experimentId + '+' + variationName;
        // console.log('LPIDcombined is: ' + params.LPIDcombined);
      }
    }

    var qsearch = window.location.search.substring(1);
    window.qpairs = qsearch?JSON.parse('{"' + qsearch.replace(/&/g, '","').replace(/=/g,'":"') + '"}', function(key, value) { return key===""?value:decodeURIComponent(value) }):{};
    var key;
    // console.log('qpairs is: ' + qpairs);
    // console.log(qpairs);
    function qExtract(variable) {
      for (var key in variable) {
        if (qpairs.hasOwnProperty(key)) {
          window.params[key] = qpairs[key];
          // console.log(params[key] + " = " + qpairs[key]);
         }
      }
    }
    qExtract(qpairs);

   if(document.getElementById('PromoID'))
